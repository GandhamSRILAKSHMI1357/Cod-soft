        "        /* Apply Inter font and rounded corners globally */\n",
        "        body {\n",
        "            font-family: 'Inter', sans-serif;\n",
        "        }\n",
        "\n",
        "        /* Custom CSS for Image Enlargement (Modal) */\n",
        "        .enlarged-modal {\n",
        "            @apply hidden fixed inset-0 bg-black bg-opacity-75 flex items-center justify-center p-4 z-50;\n",
        "        }\n",
        "\n",
        "        .enlarged-modal:target {\n",
        "            @apply block; /* Show modal when target is active */\n",
        "        }\n",
        "\n",
        "        .modal-content {\n",
        "            @apply relative bg-white p-4 rounded-lg shadow-xl max-w-4xl max-h-[90vh] overflow-auto;\n",
